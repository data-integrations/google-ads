            "off": {
              "value": "false",
              "label": "False"
            },
            "default": "true"
          }
        },
        {
          "widget-type": "toggle",
          "label": "Include Column Header",
          "name": "includeColumnHeader",
          "widget-attributes": {
            "on": {
              "value": "true",
              "label": "True"
            },
            "off": {
              "value": "false",
              "label": "False"
            },
            "default": "true"
          }
        },
        {
          "widget-type": "toggle",
          "label": "Include Report Summary",
          "name": "includeReportSummary",
          "widget-attributes": {
            "on": {
              "value": "true",
              "label": "True"
            },
            "off": {
              "value": "false",
              "label": "False"
            },
            "default": "true"
          }
        },
        {
          "widget-type": "toggle",
