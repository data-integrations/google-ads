      "properties": [
        {
          "name": "reportFormat",
          "label": "Format",
          "widget-type": "select",
          "widget-attributes": {
            "default": "CSV",
            "values": [
              "CSVFOREXCEL",
              "CSV",
              "TSV",
              "XML",
              "GZIPPED_CSV",
              "GZIPPED_XML"
            ]
          }
        },
        {
          "widget-type": "toggle",
          "label": "Include Report Header",
          "name": "includeReportHeader",
          "widget-attributes": {
            "on": {
              "value": "true",
              "label": "True"
