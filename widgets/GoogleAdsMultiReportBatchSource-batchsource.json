            "off": {
              "value": "false",
              "label": "False"
            },
            "default": "false"
          }
        },
        {
          "widget-type": "toggle",
          "label": "Include Zero Impressions",
          "name": "includeZeroImpressions",
          "widget-attributes": {
            "on": {
              "value": "true",
              "label": "True"
            },
            "off": {
              "value": "false",
              "label": "False"
            },
            "default": "false"
          }
